{
	"name":"typo3/packagebuilder",
	"description":"Create new packages with GUI modeller",
	"keywords":["php", "packagebuilder", "code generator"],
	"type":"typo3-flow-package",
	"authors":[
		{
			"name":"Nico de Haen"
		}
	],
	"require":{
		"typo3/flow": "*",
		"typo3/ice": "*",
		"typo3/parserapi": "dev-flow3_package",
		"radmiraal/emberjs": "*"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "http://github.com/nicodh/TYPO3.Ice"
		},
		{
			"type": "vcs",
			"url": "http://github.com/nicodh/php_parser_api"
		},
		{
			"type": "vcs",
			"url": "http://github.com/radmiraal/Radmiraal.Emberjs"
		}
	],
	"autoload":{
		"psr-0":{
			"TYPO3\\PackageBuilder":"Classes/"
		}
	}
}